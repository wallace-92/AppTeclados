<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.AppTeclados">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HiperXActivity"></activity>
        <activity android:name=".LogitechActivity" />
        <activity android:name=".CorsairActivity" />
        <activity android:name=".RazerActivity" />
        <activity android:name=".AsusActivity" />
        <activity android:name=".PrincipalActivity" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/Theme.Transparente" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Transparente">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>